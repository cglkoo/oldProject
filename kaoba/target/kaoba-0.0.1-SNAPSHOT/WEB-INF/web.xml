<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>jspServletFilterListener</display-name>
  <listener>
    <listener-class>com.kaokaoba.listener.LoginUserBindingListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.kaokaoba.listener.VisitListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.kaokaoba.listener.CountListener</listener-class>
  </listener>
  <filter>
    <filter-name>logFilter</filter-name>
    <filter-class>com.kaokaoba.filter.LogFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>logFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>com.kaokaoba.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>msgServlet</servlet-name>
    <servlet-class>com.kaokaoba.servlet.MsgServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>msgServlet</servlet-name>
    <url-pattern>/msg</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>regServlet</servlet-name>
    <servlet-class>com.kaokaoba.servlet.RegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regServlet</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>roleServlet</servlet-name>
    <servlet-class>com.kaokaoba.servlet.RoleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>roleServlet</servlet-name>
    <url-pattern>/role</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>1</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>